<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.crons() %>">crons</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Edit cron</a></li>
</ul>

<div class="page-header">
    <h1>Edit cron</h1>
</div>

<% var form = formFor(cron, {action: pathTo.cron(cron), method: 'PUT', id: "cron-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Update cron', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.site(cron.site()), {class: 'btn'}) %>
</div>
<%- form.end() %>
